
# Encontra o mdc(a, b) usando o algoritmo de Euclides
        PUSH   30   # a
        PUSH   24   # b
        CALL   12
        POP
        DUP
        PUSH   0
        LT
        JIF    10
        PUSH   -1
        MUL
        PRN
        END

MDC:    ALC    2
        STL    1   # b
        STL    0   # a
        RCE    1
        RCE    0
        RCE    1
        DUP
        PUSH   0
        EQ
        JIF    24
        FRE    2
        RET
        RCE    0
        RCE    1
        LT
        JIF    32
        POP
        POP
        RCE    1
        RCE    0
        SUB        # a - b
        FRE    2
        CALL   MDC # mdc(b, a - b)
        RET
