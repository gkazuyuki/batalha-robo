def murder(x){
    y = atr(x, 2);
    while (y == 1){
        atk(x);
        y = atr(x, 2);
    }
    return;
}
i = 0;
while (i < 20){
    j = 0;
    while(j < 6){
        murder(j);
        j = j + 1;
    }
    mov(0);
    i = i + 1;
}
